{
  "compilerOptions": {
    "target": "ES6", 
    "module": "NodeNext",
    "allowJs": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "moduleResolution": "NodeNext",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "environment.d.ts"],
}
